#!/bin/bash

#%# family=manual
#%# capabilities=autoconf

STYLE=LINE2
GRAPH="pid"
SECTION="system"

run_autoconf() {
	echo yes 
}

run_config() {
cat << EOF
graph_title ${GRAPH}
graph_category ${SECTION}
graph_vlabel pid
pid.label pid
pid.draw LINE2
pid.min 0
pid.info pid
EOF
}

run_()  {
echo "pid.value $$"
}

run_$1
